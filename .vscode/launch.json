    "version": "0.2.0",
    "configurations": [
        {
            "name": "demo_afgr (debug mode)",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "Flutter for web",
            "type": "dart",
            "request": "launch",
            "args": [ // flutter run -d chrome --web-browser-flag "--disable-web-security"
              "-d",
              "chrome",
              "--web-browser-flag",
              "--disable-web-security"
            ]
        },
        {
            "name": "demo_afgr (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "demo_afgr (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}